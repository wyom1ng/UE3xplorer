<Window x:Class="ue3explorer.Texture"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:ue3explorer"
        xmlns:interop="clr-namespace:System.Windows.Interop;assembly=PresentationCore"
        mc:Ignorable="d"
        Title="UE3xplorer- Texture" Height="450" Width="800">
    <Grid>
        <Image Stretch="Fill" Name="ImageHost" Margin="0">
            <Image.Source>
                <dxExtensions:D3D11Image x:Name="InteropImage" />
            </Image.Source>
        </Image>
    </Grid>
</Window>
